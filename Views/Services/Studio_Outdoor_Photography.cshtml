<!-- Start Page Title Area -->
<section class="page-title-area">
    <div class="container">
        <div class="page-title-content">
            <h1>Services Details</h1>
        </div>
    </div>

    <div class="shape2"><img src="~/asset/img/shape2.png" alt="image"></div>
    <div class="shape3"><img src="~/asset/img/shape3.png" alt="image"></div>
    <div class="shape5"><img src="~/asset/img/shape5.png" alt="image"></div>
